{
    "name": "dynamic-text-anchors",
    "description": "Dynamic Text Anchors",
    "keywords": [
        "dynamic",
        "text",
        "anchor"
    ],
    "version": "1.0.1",
    "author": "Petr Chalupa",
    "license": "SEE LICENSE IN LICENSE.md",
    "type": "module",
    "main": "./dist/lib/index.js",
    "types": "./dist/lib/index.d.ts",
    "files": [
        "dist/lib/*",
        "LICENSE.md",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "build-tsc-watch": "tsc --watch",
        "build-all": "tsc && vite build ./src/demo",
        "demo-dev": "vite ./src/demo",
