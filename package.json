        "publish-major": "npm verison major -m \"Upgrade to version %s\" && npm publish"
    },
    "dependencies": {
        "lorem-ipsum": "^2.0.8"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.2.3",
        "sass": "^1.68.0",
        "sass-loader": "^13.2.2",
        "typescript": "^5.2.2",
        "vite": "^4.4.5",
        "vue": "^3.3.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Petr-Chalupa/DynamicTextAnchors.git"
    }
